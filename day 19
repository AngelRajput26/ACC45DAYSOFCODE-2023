#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{ int a,b;
	cin>>a>>b;
	    if(a%5==0){
            if(b%5==0){
                cout<<((a/5))-((b/5))<<endl;
            }
            else{
                cout<<((a/5))-((b/5)+1)<<endl;
            }
            
        }
        else{
            if(b%5==0){
                cout<<((a/5)+1)-((b/5))<<endl;
            }   
            else{
                cout<<((a/5)+1)-((b/5)+1)<<endl;
            }
	} }
	return 0;
}
