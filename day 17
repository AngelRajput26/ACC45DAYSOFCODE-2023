#include <iostream>
using namespace std;
int main() {
	int t;
	cin>>t;
	while(t--)
	{int seat;
	cin>>seat;
	    if(seat<=10)
	    {cout<<"lower double"<<endl;}
	    else if(11<=seat and seat<=15)
	    { cout<<"lower single"<<endl;}
	    else if(17<=seat and seat<=26)
	    {cout<<"upper double"<<endl;}
	    else
	    {cout<<" upper single"<<endl;}
	}
	return 0;
}
