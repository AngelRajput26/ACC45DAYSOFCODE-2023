#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x,y,z;
	    cin>>x>>y>>z;
	    if(x+y==z||y+z==x||z+x==y){
	        cout<<"yes"<<endl;
	    }
	    else if(abs(x-y)==z||abs(y-z)==x||abs(z-x)==y){
	        cout<<"yes"<<endl;
	    }
	    else
	    cout<<"no"<<endl;
	}
	returnÂ 0;
}
