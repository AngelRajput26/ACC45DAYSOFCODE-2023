#include <iostream>
using namespace std;

int main() {
    int T;
    cin>> T;
    while(T--)
    {
        int x,y,n;
        cin>>n;
    x=1;
    y=n; 
    if(n%2 != 0)
    {
        while(x<y)
        {
            x++;
            y--;
        }
        cout<<x<<endl;
    }
    }
	
	return 0;
}
