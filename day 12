#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int a,b,c;
	    cin>>a>>b>>c;
	    if(a==b)
	    {
	        cout<<c-1<<endl;
	    }
	    else if(b/a > c)
	    {
	        cout<<0<<endl;
	    }
	    else
	    {
	        cout<<c - (b/a)<<endl;
	    }
	}
	return 0;
}
